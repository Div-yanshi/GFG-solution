class Solution {
    public static Node reverseKGroup(Node head, int k) {
        // code here
       Node prevHead=new Node(0);
      Node prevdummy=prevHead;
       Node rev=head;
     
       Node temp=head;
       int flag=0;
       while(flag==0){
       for(int i=1;temp!=null && i<=k;i++)
       {
        
       
        temp=temp.next;


       }if(temp==null)
        {
            flag=1;
          
           
        }
      
      
        Node revHead=reverse(rev,k);
        
        prevdummy.next=revHead;
        rev.next=temp;
        prevdummy=rev;
      
        rev=temp;
       
       }

        return prevHead.next;
    }
    private static Node reverse(Node temp,int k)
    {    Node prev=null;
            
        for(int i=1;temp!=null && i<=k;i++)
        {
            Node next=temp.next;
            temp.next=prev;
            prev=temp;
            temp=next;
        }
       return prev;
    }
}
    
