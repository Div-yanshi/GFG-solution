class Solution {
    public static ArrayList<Integer> findUnion(int[] a, int[] b) {
        // code here
        Set<Integer> s1=new HashSet<>();
        for(int i=0;i<a.length;i++)
        s1.add(a[i]);
        
        for(int i=0;i<b.length;i++)
        s1.add(b[i]);
        ArrayList<Integer> arr=new ArrayList<>(s1);
        return arr;
       
        
        
        
        
    }
}
